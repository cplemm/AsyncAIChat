{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Azure": {
    "SignalR": {
      "ConnectionString": "Endpoint=https://<signalr-name>.service.signalr.net;AccessKey=<signalr-access-key>;Version=1.0;"
    }
  },
  "APIM": {
    "Endpoint": "https://<your-apim-name>.azure-api.net/",
    "SubscriptionKey": "<your-apim-subscription-key>"
  }
}
